{
 "ts-node": {
  "require": ["tsconfig-paths/register"]
 },
 "compilerOptions": {
  "jsx": "react-jsx",
  "lib": ["DOM", "ES2020"],
  "module": "ES2020",
  "target": "ES2020",
  "sourceMap": false,
  "moduleResolution": "Bundler",
  "inlineSources": false,
  "esModuleInterop": true,
  "resolveJsonModule": true
 },
 "include": ["tests/**/*"],
  "exclude": ["node_modules"]
}